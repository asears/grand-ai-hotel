{
  "name": "@grand-budapest/wasm-python-playground",
  "version": "1.0.0",
  "description": "Python AST parser and analyzer running in WebAssembly with AI agent patterns",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "lint": "eslint src/js/**/*.js",
    "format": "prettier --write \"src/**/*.{js,css,html}\"",
    "type-check": "tsc --noEmit",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "keywords": [
    "webassembly",
    "wasm",
    "pyodide",
    "python",
    "ast",
    "parser",
    "code-analysis",
    "nlp",
    "github-copilot",
    "ai-agents"
  ],
  "author": "Grand Budapest Terminal",
  "license": "CC0-1.0",
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.10.0",
    "@vitest/ui": "^1.0.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "gh-pages": "^6.1.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "monaco-editor": "^0.45.0",
    "pyodide": "^0.25.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/asears/grand-ai-hotel.git",
    "directory": "examples/wasm"
  },
  "bugs": {
    "url": "https://github.com/asears/grand-ai-hotel/issues"
  },
  "homepage": "https://asears.github.io/grand-ai-hotel/wasm-playground"
}
